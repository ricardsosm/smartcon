pragma solidity >=0.4.21 <0.6.0;

contract contar {

	int private count = 0;
	address owner;
	uint cliente;
	bytes32 name;

	function incrementCounter() public {
		count += 1;
	}
	function decrementCounter() public {
		count -= 1;
	}
	function getCount() public view returns (int) {
		return count;
	}
}pragma solidity >=0.4.21 <0.6.0;

